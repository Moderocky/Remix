type jumps {

    func While {
        integer count { 0 }
        while count =! 10 {
            integer count { count + 1 }
        }
        if count =! 10 {
            system.Error {"While loop error."}
        }

        integer count { 0 }
        while true {
            integer count { count + 1 }
            break
        }
        if count =! 1 {
            system.Error {"While loop break error."}
        }
    }

    func Main {
        this.While {}
    }


}
